User Function MFIN020()

	Local cDataForm := ""

	cDataForm := SUBSTR(DTOS(DAYSUM(SE1->E1_VENCTO,5)),7,2)+SUBSTR(DTOS(DAYSUM(SE1->E1_VENCTO,5)),5,2)+SUBSTR(DTOS(DAYSUM(SE1->E1_VENCTO,5)),3,2)

Return(cDataForm)